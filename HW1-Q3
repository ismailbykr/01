#include <stdio.h>

int main() {
    int popularity, size;
    long long homeValue; //long long is used for large values

    // Getting user input
    printf("Enter popularity: ");
    scanf("%d", &popularity);

    printf("Enter size: ");
    scanf("%d", &size);

    // Calculating the home value
    homeValue = (popularity * popularity * popularity + size * size) * 10000;

    // result
    printf("Home value is: %lld TL\n", homeValue);

    return 0;
}
